<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Baumax.Domain.UserRoleName, Baumax.Contract" table="UserRoleName">
		<id name="ID" type="Int64" unsaved-value="0">
			<column name="Id" sql-type="bigint" not-null="true" unique="true" index="PK_UserRoleName"/>
			<generator class="Baumax.AppServer.PKGen, Baumax.AppServer" />
		</id>
		<property name="LanguageID" type="Int64">
			<column name="LanguageID" sql-type="bigint" not-null="true" index="UniqueRec"/>
		</property>
		<property name="Name" type="String">
			<column name="Name" length="25" sql-type="nvarchar" not-null="false"/>
		</property>
		<many-to-one name="UserRole" class="Baumax.Domain.UserRole, Baumax.Contract">
			<column name="UserRoleID" sql-type="bigint" not-null="true" index="UniqueRec"/>
		</many-to-one>
	</class>
</hibernate-mapping>
